<% include partials/header  %>


<div class="container">




<form action='/blogs/<%=blog._id%>?_method=PUT'  method='POST'>

  	<div class="form-group">
    	<label for="title">Title</label>
   	 <input type="text" class="form-control" id="title" value=<%=blog.title%> name="blog[title]">
 	 </div>

  	<div class="form-group">
    	<label for="image">Image</label>
   	 <input type="text" class="form-control" id="image" value=<%=blog.image%> name="blog[image]">
  	</div>

    <div class="form-group">
    	<label for="content">Content</label>
    <textarea class="form-control" id="content" name="blog[content]"><%=blog.content%>  </textarea>
    
    </div>

    <button class="btn btn-primary">Update</button>
    
</form>



<a href="/blogs"><button class="btn btn-primary">See all posts </button></a>




</div>


<% include partials/footer  %>